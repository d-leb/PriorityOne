{
  "name": "priorityone",
  "version": "0.1.17",
  "description": "PriorityOne is an opensource, serverless personal task management tool which can store task data in local storage.",
  "author": "David LeBlanc <github@david-leblanc.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/d-leb/PriorityOne.git"
  },
  "packageManager": "yarn@4.1.0",
  "scripts": {
    "analyze": "node scripts/analyze.mjs",
    "build": "node scripts/build.mjs",
    "eslint": "eslint --ext ts,tsx src --max-warnings=0",
    "eslint:fix": "eslint --ext ts,tsx src --fix",
    "jest": "jest",
    "jest:watch": "jest --watchAll",
    "lint": "yarn eslint && yarn stylelint && yarn prettier",
    "prettier": "prettier --check 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "review": "yarn lint && yarn test && yarn build",
    "start": "node scripts/start.mjs",
    "stylelint": "stylelint \"src/**/*.ts?\"",
    "stylelint:fix": "stylelint \"src/**/*.ts?\" --fix",
    "test": "yarn tsc && yarn jest",
    "tsc": "tsc"
  },
  "dependencies": {
    "@axe-core/react": "^4.8.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-intl": "^6.6.2",
    "react-is": "^18.2.0",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.22.3",
    "styled-components": "^6.1.8",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@swc/core": "^1.4.6",
    "@swc/helpers": "^0.5.6",
    "@swc/jest": "^0.2.36",
    "@testing-library/react": "^14.2.1",
    "@types/eslint": "^8",
    "@types/mocha": "^10.0.6",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.21",
    "@types/react-is": "^18",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "@types/webpack-bundle-analyzer": "^4",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "browserslist": "^4.23.0",
    "compression-webpack-plugin": "^11.1.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "imports-loader": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.35",
    "postcss-styled-syntax": "^0.6.4",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "svg-url-loader": "^8.0.0",
    "swc-loader": "^0.2.6",
    "typescript": "^5.4.2",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-dev-server": "^5.0.2"
  }
}
