{
  "name": "priorityone",
  "version": "0.1.2",
  "description": "PriorityOne is an opensource, serverless personal task management tool which can store task data in local storage.",
  "author": "David LeBlanc <github@david-leblanc.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/d-leb/PriorityOne.git"
  },
  "packageManager": "yarn@4.1.0",
  "scripts": {
    "analyze": "node scripts/analyze.mjs",
    "build": "node scripts/build.mjs",
    "eslint": "eslint --ext ts,tsx src --max-warnings=0",
    "eslint:fix": "eslint --ext ts,tsx src --fix",
    "lint": "yarn eslint && yarn prettier",
    "prettier": "prettier --check 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "review": "yarn lint && yarn test && yarn build",
    "start": "node scripts/start.mjs",
    "test": "yarn tsc",
    "tsc": "tsc"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@swc/core": "^1.4.2",
    "@swc/helpers": "^0.5.6",
    "@types/eslint": "^8",
    "@types/react": "^18.2.59",
    "@types/react-dom": "^18.2.19",
    "@types/react-is": "^18",
    "@types/webpack-bundle-analyzer": "^4",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "browserslist": "^4.23.0",
    "compression-webpack-plugin": "^11.0.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "imports-loader": "^5.0.0",
    "prettier": "^3.2.5",
    "svg-url-loader": "^8.0.0",
    "swc-loader": "^0.2.6",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-dev-server": "^5.0.2"
  }
}
